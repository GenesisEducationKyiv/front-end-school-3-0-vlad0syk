{
  "name": "genesis-project",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "cross-env PORT=3000 vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "test:e2e": "playwright test",
    "test:component": "playwright test src/tests/component/",
    "test:integration": "vitest src/tests/integration/",
    "test:unit": "vitest src/tests/unit/",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:component && npm run test:e2e",
    "dev:test": "concurrently \"npm run dev\" \"wait-on http://localhost:3000 && npm run test:e2e\"",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.1.1",
    "@mobily/ts-belt": "^3.13.1",
    "@tailwindcss/vite": "^4.1.4",
    "@tanstack/react-query": "^5.74.4",
    "@tanstack/react-query-devtools": "^5.74.6",
    "@testing-library/react": "^16.3.0",
    "@types/lodash": "^4.17.16",
    "@vitest/coverage-v8": "^3.2.4",
    "lodash": "^4.17.21",
    "neverthrow": "^8.2.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.57.0",
    "react-router-dom": "^7.6.2",
    "react-toastify": "^11.0.5",
    "tailwind": "^2.3.1",
    "zod": "^3.25.42",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.28.0",
    "@playwright/experimental-ct-react": "^1.53.1",
    "@playwright/test": "^1.53.1",
    "@types/node": "^24.0.3",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.6.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.28.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "nx": "21.2.1",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.33.0",
    "vite": "^6.3.1",
    "vitest": "^3.2.4"
  },
  "nx": {}
}
