    "eslint-plugin-storybook": "^9.0.17",
    "globals": "^16.0.0",
    "jsdom": "^26.1.0",
    "playwright": "^1.54.1",
    "rollup-plugin-visualizer": "^6.0.3",
    "storybook": "^9.0.17",
